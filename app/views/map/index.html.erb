<div class="span12">
<% if signed_in? %>
  <% provide(:title, 'Home')%>
  <div class="row-fluid">
    <div class="span9"><%= render 'microposts/form' %></div>
  </div>
  <div class="row-fluid news">
    <div class="span9">
      <ul class="nav nav-tabs" style="margin:0; padding:0;">
        <li class="active"><%=link_to("Map View", map_index_path(u_id: @user.id), {remote: true})%></li>
        <li><%=link_to("Feeds", microposts_path(u_id: @user.id, posts: "11"), {remote: true})%></li>
      </ul>
      <div class="tab-content" style="position:relative;">
	  	  <%= render "map/map" %>
      </div>
    </div>
  </div>

<% else %>

	<% provide(:title) %>
    <h1>Map#index</h1>
    <p>Find me in app/views/map/index.html</p>
    <p>Registion link </p>
    <%= link_to "Sign up now!", signup_path, class: "signup_button round" %>

<% end %>
</div>






