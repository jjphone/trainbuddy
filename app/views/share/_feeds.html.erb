<div class="span9">

  <% @posts ||= "11" %>
  <div class="row-fluid">
    <div class="span12 menu-bar">
      <div class="menu-tag"><%= gen_post_menu(@user.id, @posts.first, @posts.last, @url.path) %></div>
    </div>
  </div>
<!-- menu-bar separator  -->
  <div id="stops_times"><%= render 'share/pop-stops' if @stops%></div>
  
  <div class="row-fluid">
    <div class="tab-content feeds span12">
    
      <div class="row-fluid">
        <div class="span12">
          <% if @feed_items && @feed_items.any? %>
            <ul class="list-items posts">
              <%= render partial: 'share/feed_item', collection: @feed_items %>
            </ul>
          <% else %>
            <h5 style="margin: 2px 0 100px 20px;">NO feed item found.</h5>
          <% end %>
        </div>
      </div>

      <div class="row-fluid">
        <% if @feed_items %><div class="span8"><%= will_paginate @feed_items %></div><%end%>
      </div>

    </div>
  </div>

</div>