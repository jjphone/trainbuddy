<% provide(:title, 'Edit user') %>
<div class="row-fluid">
  <div class="span11 offset1"><h1>Edit User</h1></div>
</div>


<%= form_for(@user, html: {multipart: true}) do |f| %>

  <%= render 'fields', f: f %>

  <div class="row-fluid" style="border-top: 1px solid #ccc; margin-top: 20px;">
	<div class="action span3 offset1" ><%= f.submit "Update",  class: "btn-large " %></div>
	<div class="span5">Config Matching and route pre-definitions</div>
  </div>

<% end %>








